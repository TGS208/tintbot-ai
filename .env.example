REACT_APP_ELASTICSEARCH_ENDPOINT=https://your-cluster.es.region.aws.found.io:9243
REACT_APP_ELASTICSEARCH_API_KEY=your_elasticsearch_api_key_here

# Netlify Functions
NETLIFY_FUNCTIONS_ENDPOINT=https://your-site.netlify.app/.netlify/functions

# Google Services (Optional)
GOOGLE_PROJECT_ID=your-google-project-id
GOOGLE_PRIVATE_KEY_ID=your-private-key-id
